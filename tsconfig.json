{
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "noEmit": true,
        "skipLibCheck": true,
        "strict": true,
        "target": "esnext",
        "allowImportingTsExtensions": true,
        "outDir": "dist"
    }
}
