{
    "compilerOptions": {
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "noEmit": true,
        "skipLibCheck": true,
        "strict": true,
        "target": "ES2023",
        "allowImportingTsExtensions": true
    }
}
